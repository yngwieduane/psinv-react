generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Property {
  id                  Int      @id @default(autoincrement())
  propertyID          String   @unique
  propertyName        String
  propertyType        String?
  propertyPlan        String?
  slug                String?
  country             String?
  city                String?
  district            String?
  community           String?
  minPrice            Float?
  maxPrice            Float?
  enMarketingTitle    String?
  enPropertyOverView  String?
  mapLatitude         Float?
  mapLongitude        Float?
  createdAt           DateTime @default(now())
}